{
  "$schema": "https://opencode.ai/config.json",
  "model": "llama-local/qwen3-coder",
  "theme": "night-owl",
  "provider": {
    "llama-local": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "llama-server (local)",
      "env": [],
      "options": {
        "baseURL": "http://127.0.0.1:11444/v1",
        "apiKey": "not-needed"
      },
      "models": {
        "qwen3-coder": {
          "name": "Qwen3-Coder-30B-A3B",
          "tool_call": true,
          "attachment": false,
          "temperature": true,
          "limit": {
            "context": 49152,
            "output": 8192
          }
        }
      }
    }
  },
  "permission": {
    "bash": "ask",
    "edit": "allow",
    "read": "allow",
    "write": "allow"
  },
  "keybinds": {
    "leader": "ctrl+a"
  }
}
